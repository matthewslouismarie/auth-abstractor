{
    "name": "matthewslouismarie/auth-abstractor",
    "type": "library",
    "description": "Complete abstraction of credential and U2F authentication",
    "keywords": ["authentication", "U2F", "middleware", "password"],
    "homepage": "https://github.com/matthewslouismarie/auth-abstractor",
    "authors": [
        {
            "name": "Louis-Marie Matthews",
            "email": "matthewslouismarie@gmail.com",
            "homepage": "https://matthewslouismarie.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "LM\\": "src/LM",
            "Tests\\LM\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "friendsofphp/php-cs-fixer": "^2.11",
        "infection/infection": "^0.8.1"
    },
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "^1.0",
        "twig/twig": "^2.0",
        "symfony/asset": "^4.0",
        "php-di/php-di": "^6.0",
        "symfony/form": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/twig-bridge": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/config": "^4.0",
        "symfony/http-foundation": "^4.0",
        "symfony/psr-http-message-bridge": "^1.0",
        "psr/container": "^1.0",
        "firehed/u2f": "dev-master",
        "symfony/validator": "^4.0",
        "zendframework/zend-diactoros": "^1.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
